{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "properties": {
        "GenericKey1": {
            "type": "string",
            "description": "The first generic key for the appointment"
        },
        "GenericKey2": {
            "type": "string",
            "description": "The second generic key for the appointment"
        },
        "From": {
            "type": "string",
            "format": "date-time",
            "description": "The start date and time of the appointment"
        },
        "To": {
            "type": "string",
            "format": "date-time",
            "description": "The end date and time of the appointment"
        },
        "WholeDay": {
            "type": "boolean",
            "description": "Indicates if the appointment is a whole day event"
        },
        "Recurring": {
            "type": "boolean",
            "description": "Indicates if the appointment is recurring"
        },
        "RecurringRule": {
            "type": "string",
            "description": "The recurring rule for the appointment"
        },
        "RecurringException": {
            "type": "string",
            "description": "The recurring exception for the appointment"
        },
        "Note": {
            "type": "string",
            "description": "Additional notes for the appointment"
        },
        "Category": {
            "type": "string",
            "description": "The category of the appointment"
        },
        "Flag": {
            "type": "string",
            "description": "The flag for the appointment"
        },
        "Description": {
            "type": "string",
            "description": "The description of the appointment"
        }
    },
    "required": [
        "GenericKey1",
        "GenericKey2",
        "From",
        "To",
        "WholeDay",
        "Recurring",
        "RecurringRule",
        "RecurringException",
        "Note",
        "Category",
        "Flag",
        "Description"
    ]
}
