{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "ServiceTechnician",
    "type": "object",
    "properties": {
        "technicianID": {
            "type": "string",
            "description": "Property. Technician Identification Code."
        },
        "organization": {
            "type": "string",
            "description": "Property. Organization to which the technician belongs (PRIMA branch or external office)."
        },
        "hasSkill": {
            "type": "array",
            "items": {
                "$ref": "https://smart-data-models.github.io/dataModel.PredictiveMaintenace/MaintenanceSkill/schema.json"
            },
            "description": "Property. Description of capabilities and experiences."
        },
        "toolProvided": {
            "type": "array",
            "items": {
                "type": "string",
                "description": "Tool name."
            },
            "description": "Property. Technical equipment (special equipment not always provided for all teams)."
        },
        "geographicalArea": {
            "type": "string",
            "description": "Property. Definition of the area and location."
        },
        "referenceAddress": {
            "type": "string",
            "description": "Property. Starting position (Office and Home)."
        },
        "calendar": {
            "type": "array",
            "items": {
                "type": "string",
                "format": "date",
                "description": "Calendar of activities for the period already planned."
            },
            "description": "Property. List of planned activities."
        },
        "historicalService": {
            "type": "array",
            "items": {
                "type": "string",
                "format": "date",
                "description": "Managed service."
            },
            "description": "Property. List of managed services."
        }
    },
    "required": [
        "technicianID",
        "organization",
        "hasSkill",
        "toolProvided",
        "geographicalArea",
        "referenceAddress",
        "calendar",
        "historicalService"
    ]
}